  <collision>
    <geometry>
      <box size="0.2 0.05 0.05"/>
    </geometry>
    <origin xyz="0.1 0.03 0" rpy="0.0 0.0 0.0"/>
  </collision>
   <inertial>
    <mass value="1"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1.0"/>
  </inertial>
</link>

<link name="left_forearm">
  <visual>
    <geometry>
      <box size="0.2 0.05 0.05"/>
    </geometry>
    <origin xyz="0.1 0.0 -0.002" rpy="0.0 0.0 0.0"/>
  </visual>


  <inertial>
    <mass value="1"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1.0"/>
  </inertial>
  
  <collision>
    <geometry>
      <box size="0.2 0.05 0.05"/>
    </geometry>
    <origin xyz="0.1 0.0 -0.002" rpy="0.0 0.0 0.0"/>
  </collision> 
</link>

