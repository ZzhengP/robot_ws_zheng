<joint name="joint_chest_y" type="fixed">
    <parent link = "link_virtual"/>
    <child link="base_corps_link_2"/>
    <origin rpy ="0. 0. 0." xyz = "0.0 0.0 0."/>
</joint>

<joint name="joint1" type="revolute">
  <origin xyz="0.00 0.075 0.1" rpy="0. 1.57 0.0"/>
  <parent link="base_corps_link_2"/>
  <child link="left_shoulder_psi_link"/>
  <axis xyz="0 0 1"/>
  <limit lower="-1.57" upper="1.57" effort="100.0" velocity="10.0"/>
</joint>

<joint name="joint2" type="revolute">
  <origin xyz="0.0 0. 0." rpy="0. 0. 0.0"/>
  <parent link="left_shoulder_psi_link"/>
  <child link="left_arm_shoulder"/>
  <axis xyz="0 -1 0"/>
  <limit lower="-1.57" upper="1.57" effort="100.0" velocity="10.0"/>
</joint>

<joint name="joint3" type="revolute">
  <origin xyz="0.2 0.025 0." rpy="1.57 0. 0.0"/>
  <parent link="left_arm_shoulder"/>
  <child link="left_forearm"/>
  <axis xyz="0 0 1"/>
  <limit lower="-1.57" upper="1.57" effort="100.0" velocity="10.0"/>
</joint>


<!-- **************************** Link Properties ********************************* -->
<link name="world"/> 

<link name="base_corps_link_1">
  <visual>
    <geometry>  
      <box size="0.15 0.15 0.15"/>
    </geometry>
    <origin xyz="0.0 0.0 0.075" rpy="0.0 0.0 0.0"/>
  </visual>

  <collision>
    <origin xyz="0.0 0.0 0.075" rpy="0.0 0.0 0.0"/>
    <geometry>  
      <box size="0.15 0.15 0.15"/>
    </geometry>
  </collision>

  <inertial>
    <mass value="20"/>
    <inertia ixx="10.0" ixy="0.0" ixz="0.0" iyy="10" iyz="0.0" izz="10.0"/>
  </inertial>
</link>

<link name="link_virtual">
  <visual>
    <geometry>
      <cylinder radius="0.01" length="0.075"/>
    </geometry>
    <origin xyz="0.0 0.0 0.0" rpy="-1.57 0.0 0.0"/>
  </visual>
  <inertial>
    <mass value="0.01"/>
    <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
  </inertial>
</link>


<link name="base_corps_link_2">
  <visual>
    <geometry>
      <box size="0.15 0.15 0.15"/>
    </geometry>
    <origin xyz="0. 0. 0.075" rpy="0.0 0.0 0.0"/>
  </visual>

  <collision>
    <origin xyz="0. 0. 0.075" rpy="0.0 0.0 0.0"/>
    <geometry>  
      <box size="0.15 0.15 0.15"/>
    </geometry>
  </collision>

  <inertial>
    <mass value="1"/>
    <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1.0"/>
  </inertial>
</link>



<link name="left_shoulder_psi_link" >
  <visual>
    <geometry>
      <cylinder radius="0.01" length="0.075"/>
    </geometry>
    <origin xyz="0.0 0.0 0.0" rpy="0 0.0 0.0"/>
  </visual>
  <inertial>
    <mass value="0.01"/>
    <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
  </inertial>
</link>
