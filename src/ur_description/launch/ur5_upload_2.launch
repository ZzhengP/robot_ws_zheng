<launch>
<arg name="limited" default="false" doc="If true, limits joint range [-PI, PI] on all joints." />

<param unless="$(arg limited)" name="robot_description_r2" command="$(find xacro)/xacro --inorder '$(find ur_description)/urdf/ur5_robot.urdf.xacro'" />
<param if="$(arg limited)" name="robot_description_r2" command="$(find xacro)/xacro --inorder '$(find ur_description)/urdf/ur5_joint_limited_robot.urdf.xacro'" />
</launch>
